<footer class="footer">
  <nav class="footer-nav-top">
    <ul class="footer-nav-top-ul" role="list">
      {% for link in linklists.footer-third-menu.links -%}
        <li class="footer-nav-top__li-link">
          {{ link.title | link_to: link.url }}
        </li>
      {%- endfor %}
    </ul>
  </nav>
  <nav class="footer-nav-middle">
    <ul class="footer-nav-middle-ul" role="list">
      {% for link in linklists.footer.links -%}
        <li class="footer-nav-middle__li-link">
          {{ link.title | link_to: link.url }}
        </li>
      {%- endfor %}
    </ul>
  </nav>
  <div class="footer-bottom">
    <div class="footer-bottom__social">
      <div>
        <a class="footer-bottom__social-fb" href="#">
          {% render 'icon-facebook' %}
        </a>
      </div>
      <div>
        <a href="#" class="footer-bottom__social-twi">
          {% render 'icon-twitter' %}
        </a>
      </div>
      <div>
        <a href="#" class="footer-bottom__social-ins">
          {% render 'icon-instagram' %}
        </a>
      </div>
      <div>
        <a href="#" class="footer-bottom__social-you">
          {% render 'icon-youtube' %}
        </a>
      </div>
    </div>
    <div class="footer-bottom__payment">
      {% render 'icon-paypal' %}
      {% render 'icon-visa' %}
      {% render 'icon-mastercard' %}
      {% render 'icon-klarna' %}
    </div>
    <div class="footer-bottom__menu">
      <nav class="footer-nav-bottom">
        <ul class="footer-nav-bottom-ul" role="list">
          {% for link in linklists.footer-second-menu.links -%}
            <li class="footer-nav-bottom__li-link">
              {{ link.title | link_to: link.url }}
            </li>
          {%- endfor %}
        </ul>
      </nav>
    </div>
  </div>
</footer>